<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="menu">
    <!-- 01.메뉴 전체 목록 조회 -->
    <select id="menulistAll" resultType="com.way.free.model.menu">
    	SELECT
    		majorca,
    		mediumca,
    		minorca
    	FROM
    		menu
    	ORDER BY
    		canum desc
    </select>
    </mapper>